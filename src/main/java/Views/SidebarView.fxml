<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
   <left>
      <VBox prefHeight="200.0" prefWidth="100.0" style="-fx-border-color: black;" BorderPane.alignment="CENTER">
         <children>
            <BorderPane prefHeight="600.0" prefWidth="98.0">
               <top>
                  <Button fx:id="homeButton" mnemonicParsing="false" onAction="#OnClickHome" prefHeight="69.0" prefWidth="101.0" text="HOME" BorderPane.alignment="CENTER">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Button>
               </top>
               <center>
                  <Button fx:id="feedButton" mnemonicParsing="false" onAction="#OnClickFeed" prefHeight="69.0" prefWidth="101.0" text="FEED" BorderPane.alignment="CENTER">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Button>
               </center>
               <bottom>
                  <Button fx:id="profileButton" mnemonicParsing="false" onAction="#OnClickProfile" prefHeight="69.0" prefWidth="101.0" text="PROFILE" BorderPane.alignment="CENTER">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Button>
               </bottom>
            </BorderPane>
         </children>
      </VBox>
   </left>
</BorderPane>
